{
  "name": "todo",
  "version": "1.0.0",
  "description": "",
  "author": "Jason Etcovitch <jasonetco@gmail.com> (https://github.com/jasonetco/todo)",
  "license": "ISC",
  "repository": "https://github.com/JasonEtco/todo.git",
  "scripts": {
    "dev": "nodemon",
    "start": "node ./index.js",
    "test": "jest --coverage && standard",
    "test:update": "jest -u"
  },
  "standard": {
    "env": {
      "jest": true
    }
  },
  "bin": {
    "todo": "./bin/todo.js"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/tests/fixtures/",
      "<rootDir>/tests/helpers.js",
      "<rootDir>/tests/setup.js"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/tests/helpers.js",
      "<rootDir>/tests/setup.js",
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@octokit/rest": "^15.1.7",
    "chalk": "^2.3.1",
    "commander": "^2.14.1",
    "hbs": "^4.0.1",
    "heapdump": "^0.3.12",
    "parse-diff": "^0.5.1",
    "probot": "github:JasonEtco/probot#memory-leak"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "nodemon": "^1.18.6",
    "smee-client": "^1.0.1",
    "standard": "^11.0.0"
  },
  "engines": {
    "node": "10.13.x",
    "npm": "6.x.X"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
